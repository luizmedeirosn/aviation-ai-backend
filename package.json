{
  "name": "aviation-ai",
  "version": "1.0.0",
  "description": "Technical challenge for Aviation AI backend application.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "node --loader ts-node/esm src/server.ts",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test}/**/*.{ts,js,json}\"",
    "ci": "npm ci",
    "clean-install": "rm -rf node_modules && npm install",
    "docker-mongo-up": "docker compose -f docker-compose.yml up -d",
    "docker-mongo-down": "docker compose -f docker-compose.yml down"
  },
  "keywords": [
    "typescript",
    "graphql",
    "express",
    "mongodb",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/graphql": "^14.2.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.8.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "apollo-server-express": "^3.13.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.2",
    "graphql": "^16.11.0",
    "mongoose": "^8.19.1",
    "openai": "^6.3.0",
    "pdf-parse": "^2.4.3"
  }
}
